{
    
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "window.zoomLevel": 0,
    "code-runner.executorMap": {
        "typescript": "clear && npx ts-node --files",
    },
    "code-runner.runInTerminal": true
}